
public class Date {
	
	int month;
	int year;
	int day;
	
	
	void SetMonth(){this.month = month;}
	int GetMonth(int month){return month;}
	
	void SetYear(){this.year = year;}
	int GetYear(int year){return year;}
	
	void SetDay(){this.day = day;}
	int GetDay(int day){return day;}
	
	public Date (int month, int day, int year){month=month; day = day; year = year;}
	
	displayDate()
	{
		System.out.printf(%d "/" %d "/" %d, month , day, year);
	}

}
